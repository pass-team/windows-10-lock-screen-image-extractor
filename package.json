{
  "name": "windows-10-lock-screen-image-extractor",
  "version": "1.0.0",
  "main": "index.js",
  "author": "duyphaphach <duy@passcareer.io>",
  "license": "MIT",
  "scripts": {
    "lint:scripts": "npx eslint .",
    "lint:scripts:fix": "npx eslint . --fix",
    "test": "npx jest --config='config/jest/jest.js'",
    "test:unit": "npx jest --config='config/jest/unit/jest.js' --runInBand",
    "test:unit:coverage": "npx jest --config='config/jest/unit/jest.js' --runInBand --coverage && codacy-coverage < ./coverage/lcov.info --debug",
    "dev": "npx webpack -w --mode development",
    "build:win": "yarn run build:prod && npx pkg ./build/get-lock-screen-image.js --targets node10-win-x64 --output=build/get-lock-screen.exe --config ./package.json",
    "build:win:zip": "yarn run build:win && node ./config/archiver/archiver.js",
    "build:dev": "npx webpack --mode development",
    "build:prod": "npx webpack --mode production",
    "test:integration": "npx jest --config='config/jest/integration/jest.js' --runInBand"
  },
  "bin": {
    "get-lock-screen": "bin/get-lock-screen-image.js"
  },
  "dependencies": {
    "caporal": "1.4.0",
    "casex": "3.0.0",
    "chalk": "4.1.0",
    "enquirer": "2.3.6",
    "find-up": "4.1.0",
    "hasha": "5.2.0",
    "image-size": "0.8.3",
    "jsonfile": "6.0.1",
    "minimist": "1.2.5",
    "ora": "4.0.4",
    "strip-ansi": "6.0.0",
    "wallpaper": "4.4.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "archiver": "4.0.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "chokidar-cli": "2.1.0",
    "clean-webpack-plugin": "3.0.0",
    "codacy-coverage": "3.4.0",
    "copy-webpack-plugin": "6.0.3",
    "eslint": "7.4.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "26.1.0",
    "lodash-webpack-plugin": "0.11.5",
    "nixt": "0.5.1",
    "pkg": "4.4.9",
    "shebang-loader": "0.0.1",
    "terser-webpack-plugin": "3.0.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  }
}
